<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN" 
  "http://castor.exolab.org/mapping.dtd">
<mapping>
  <class name="org.openthinclient.server.ConfigurationDescriptor" verify-constructable="false">
    <map-to xml="configuration"/>
    <field name="created" set-method="setCreated" get-method="getCreated">
      <bind-xml name="created" node="attribute"/>
    </field>
    <field name="createdBy" set-method="setCreatedBy" get-method="getCreatedBy">
      <bind-xml name="user" node="attribute"/>
    </field>
    <field name="templateVersion" set-method="setTemplateVersion" get-method="getTemplateVersion">
      <bind-xml name="template-version" node="attribute"/>
    </field>
    <field name="description" set-method="setDescription" get-method="getDescription">
      <bind-xml name="description" node="element"/>
    </field>
  </class>
</mapping>