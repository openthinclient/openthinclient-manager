<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>openthinclient.org</display-name>
  <description>
     openthinclient.org servlets
  </description>
  <servlet>
    <servlet-name>FileServiceServlet</servlet-name>
    <servlet-class>org.openthinclient.servlet.FileServiceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileServiceServlet</servlet-name>
    <url-pattern>/files/*</url-pattern>
  </servlet-mapping>
</web-app>
