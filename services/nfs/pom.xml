	<!-- Declares to maven what class is the entry point to launch -->
	<!-- Like a command line parameter: -Dexec.mainClass="<class_name>" -->
	<!-- Command line arguments: -Dexec.args="argument1" ... -->
	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>exec-maven-plugin</artifactId>
				<version>1.2.1</version>
				<executions>
					<execution>
						<goals>
							<goal>java</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<mainClass>NFSServerMain</mainClass>
				</configuration>
			</plugin>
		</plugins>
	</build>

