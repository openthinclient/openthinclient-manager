<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:nfs xmlns:ns2="http://www.openthinclient.org/ns/manager/service/nfs/1.0">
    <nfsPort>2069</nfsPort>
    <nfsProgramNumber>100003</nfsProgramNumber>
    <mountdPort>0</mountdPort>
    <mountdProgramNumber>100005</mountdProgramNumber>

    <!-- The default Portmapper port is 111. But to allow this service to startup within an development environment,
    we're specifiying 10111, which is allowed to bound by non root-processes -->
    <portmapPort>10111</portmapPort>
    <portmapProgramNumber>100000</portmapProgramNumber>
    <flushInterval>300</flushInterval>
</ns2:nfs>
